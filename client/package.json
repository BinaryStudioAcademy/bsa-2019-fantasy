{
    "name": "client",
    "version": "0.1.0",
    "private": true,
    "proxy": "http://localhost:5001/",
    "scripts": {
        "start": "npm-run-all --parallel watch:css start:react",
        "build": "npm-run-all build:css build:react",
        "build:css": "postcss src/styles/tailwind.css -o src/generated/tailwind.output.css",
        "watch:css": "postcss src/styles/tailwind.css -o src/generated/tailwind.output.css -w",
        "start:react": "react-scripts start",
        "build:react": "react-scripts build",
        "test": "react-scripts test",
        "eject": "react-scripts eject"
    },
    "browserslist": [
        ">0.2%",
        "not dead",
        "not ie <= 11",
        "not op_mini all"
    ],
    "dependencies": {
        "classnames": "^2.2.6",
        "connected-react-router": "^6.5.2",
        "moment": "^2.24.0",
        "prop-types": "^15.7.2",
        "query-string": "^6.4.2",
        "react": "^16.8.6",
        "react-dom": "^16.8.6",
        "react-infinite-scroller": "^1.2.4",
        "react-redux": "^7.0.1",
        "react-router-dom": "^5.0.0",
        "react-scripts": "^3.0.1",
        "redux": "^4.0.1",
        "redux-thunk": "^2.3.0",
        "socket.io-client": "^2.2.0",
        "validator": "^10.11.0"
    },
    "devDependencies": {
        "@types/classnames": "^2.2.9",
        "@types/moment": "^2.13.0",
        "@types/react": "^16.8.24",
        "@types/react-dom": "^16.8.5",
        "@types/react-infinite-scroller": "^1.2.1",
        "@types/react-redux": "^7.1.1",
        "@types/react-router-dom": "^4.3.4",
        "@types/redux-thunk": "^2.1.0",
        "@types/socket.io-client": "^1.4.32",
        "@types/validator": "^10.11.2",
        "@typescript-eslint/eslint-plugin": "^1.13.0",
        "@typescript-eslint/parser": "^1.13.0",
        "autoprefixer": "^9.6.1",
        "eslint-config-airbnb": "^17.1.0",
        "eslint-config-prettier": "^6.0.0",
        "eslint-plugin-import": "^2.17.2",
        "eslint-plugin-jsx-a11y": "^6.2.1",
        "eslint-plugin-prettier": "^3.1.0",
        "eslint-plugin-react": "^7.14.3",
        "eslint-plugin-react-hooks": "^1.6.1",
        "node-sass": "^4.12.0",
        "npm-run-all": "^4.1.5",
        "postcss-cli": "^6.1.3",
        "prettier": "^1.18.2",
        "prettier-eslint": "^9.0.0",
        "redux-devtools-extension": "^2.13.8",
        "tailwindcss": "^1.1.0",
        "typescript": "^3.5.3"
    }
}
